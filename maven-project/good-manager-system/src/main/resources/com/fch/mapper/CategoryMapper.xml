<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fch.mapper.CategoryMapper">

    <select id="selectByCid" resultType="com.fch.bean.Category">
        select cid, cname from category where cid = #{cid}
    </select>
    <select id="getAllCategory" resultType="com.fch.bean.Category">
        select * from category
    </select>
</mapper>